{
    "builds": [
      {
        "src": "storefront/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "headers": { "cache-control": "s-maxage=31536000, immutable" },
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "storefront/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SECRET_KEY": "@django_secret_key",
      "DATABASE_URL": "@database_url"
    }
  }